<style lang="scss">
  @use '../styles/abstracts' as *;

  .nav {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: $spacing-unit * 2;
    position: relative;
    @include c64-pro-mono;
  }

  .nav__logo {
    width: 100px;
  }

  .nav__toggle {
    display: none;
    background: none;
    border: none;
    color: $color-text;
    font-size: 1.5rem;
    cursor: pointer;
  }

  .nav__links {
    display: flex;
    align-items: center;
    gap: $spacing-unit * 4;

    a {
      font-size: 1.01rem;
      text-decoration: none;
      text-transform: uppercase;
      vertical-align: middle;

      &:first-child {
        display: none;
      }

      &:hover {
        color: $color-accent;
      }
    }

    .theme__toggle {
      width: 45px;
      cursor: pointer;
      background: none;
      border: none;
      color: var(--color-text);

      &:hover {
        color: $color-accent;
      }
    }
  }

  @media (max-width: 768px) {
    .nav__toggle {
      display: block;
    }

    .nav__links {
      display: none;
      position: absolute;
      top: 100%;
      left: 0;
      right: 0;
      margin: 0;
      flex-direction: column;
      background: $color-bg;
      padding: 0 ($spacing-unit * 2) ($spacing-unit * 2);
      
      &[data-open] {
        display: flex !important;
      }

      a:first-child {
        display: block;
      }  
    }

    .nav__logo {
      text-transform: uppercase;
      text-decoration: none;
      img {
        display: none;
      }
    }
  }
</style>

<script>
  const navToggle = document.querySelector('.nav__toggle');
  const links = document.querySelector('.nav__links');
  navToggle?.addEventListener('click', () => {
    const isOpen = links?.toggleAttribute('data-open');
    navToggle.setAttribute('aria-expanded', String(isOpen));
  });

  const themeToggle = document.querySelector('.theme__toggle');
  const saved = localStorage.getItem('theme');
  const systemTheme = window.matchMedia('(prefers-color-scheme: dark)').matches ? 'dark' : 'light';
  function applyTheme(setting: string) {
    const resolved = setting === 'system' ? systemTheme : setting;
    document.documentElement.setAttribute('data-theme', resolved);
    document.documentElement.setAttribute('data-theme-setting', setting);
  }
  applyTheme(saved ?? 'system');
  themeToggle?.addEventListener('click', () => {
    const current = document.documentElement.getAttribute('data-theme-setting');
    const next = current === 'light' ? 'dark'
             : current === 'dark' ? 'system'
             : current === 'system' ? 'c64'
             : 'light';
    applyTheme(next);
    localStorage.setItem('theme', next);
  });
</script>